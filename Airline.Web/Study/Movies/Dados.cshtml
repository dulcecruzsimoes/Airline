@model Airline.Web.Data.MyViewModel

@{ ViewData["Title"] = "Dados"; }

<div class="site-section" id="next">
    <div class="container">
        <h2>Create Department</h2>

        <br />
        <div class="row">
            <div class="col-md-4">
                <form asp-action="Dados">
                    <div class="form-horizontal">
                        <h4>Movie</h4>
                        <hr />
                        <div asp-validation-summary="All" class="text-danger"></div>
                        <div class="form-group">
                            <label asp-for="Begin" class="col-md-2 control-label"></label>
                            <div class="col-md-10">
                                <input asp-for="Begin" class="form-control" />
                                <span asp-validation-for="Begin" class="text-danger"></span>
                            </div>
                        </div>
                        <!-- <snippet_ReleaseDate> -->
                        <div class="form-group">
                            <label asp-for="End" class="col-md-2 control-label"></label>
                            <div class="col-md-10">
                                <input asp-for="End" class="form-control" />
                                <span asp-validation-for="End" class="text-danger"></span>
                            </div>
                        </div>
                        <!-- </snippet_ReleaseDate> -->
                       
                        <div class="form-group">
                            <div class="col-md-offset-2 col-md-10">
                                <input type="submit" value="Create" class="btn btn-default" />
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
